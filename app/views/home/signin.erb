<h1>Sign in</h1>

      <form method='post' action='/auth/google_oauth2'>
        <input type="hidden" name="authenticity_token" value='<%= request.env["rack.session"]["csrf"] %>'>
        <button type='submit' class="btn btn-lg btn-primary mb-3" ><i class="fa fa-google"></i> Sign in with Google</button>
      </form>
      <form method='post' action='/auth/github'>
        <input type="hidden" name="authenticity_token" value='<%= request.env["rack.session"]["csrf"] %>'>
<button type='submit' class="btn btn-lg btn-primary mb-3" href="/auth/github"><i class="fa fa-github-alt"></i> Sign in with GitHub</button>
      </form>
